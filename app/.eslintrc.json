{
  "extends": [
    "next/core-web-vitals"
  ],
  "plugins": [
    "unused-imports",
    "simple-import-sort"
  ],
  "rules": {
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2,
        "maxEOF": 0
      }
    ],
    "eol-last": [
      "error",
      "always"
    ],
    // "no-unused-vars": "off", // checked by eslint-plugin-unused-imports
    "unused-imports/no-unused-imports": "error",
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "error"
  }
}